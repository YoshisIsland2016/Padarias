    <style type="text/css">
      body { background: url(view/admin/img/bg-login.jpg) !important; }
    </style>
  </head>
  <body>
    <div class="container-fluid-full">
      <div class="row-fluid">
        <div class="row-fluid">
          <div class="login-box">
            <div class="icons">
              <a href="index.html"><i class="halflings-icon home"></i></a>
              <a href="#"></a>
            </div>
            <h2>Entre na sua conta</h2>
            <form class="form-horizontal" action="efetuarLogin"
            method="post">
              <fieldset>
                <div class="input-prepend" title="Username">
                  <span class="add-on">
                    <i class="halflings-icon user"></i>
                  </span>
                  <input class="input-large span10" name="login" id="username" type="text"
                  placeholder="Login" required="required">
                </div>
                <div class="clearfix"></div>
                <div class="input-prepend" title="Password">
                  <span class="add-on">
                    <i class="halflings-icon lock"></i>
                  </span>
                  <input class="input-large span10" name="senha" id="password" type="password"
                  placeholder="Senha" required="required">
                </div>
                <div class="clearfix"></div>
                <div class="button-login">
                  <button type="submit" class="btn btn-primary">Login</button>
                </div>
                <div class="clearfix"></div>
                <hr>
              </fieldset>
            </form>
          </div>
          <!--/span-->
        </div>
        <!--/row-->
      </div>
      <!--/.fluid-container-->
    </div>
    <!--/fluid-row-->
